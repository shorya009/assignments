part A start

ans 1-SELECT LOCATION FROM SANDWICHES WHERE FILLING IN (SELECT FILLING FROM TASTES WHERE NAME = Jones);

ans 2-SELECT s.location FROM SANDWICHES s LEFT JOIN TASTES t ON s.FILLING = t.FILLING WHERE t.NAME = 'Jones';

ans 3-SELECT k.LOCATION,sum(k.sum) AS 'People Who Can Eat' FROM (SELECT t.NAME,s.LOCATION,COUNT(DISTINCT s.LOCATION) as 'sum' FROM SANDWICHES s LEFT JOIN TASTES t ON s.FILLING = t.FILLING GROUP BY s.LOCATION,t.NAME) k GROUP BY k.LOCATION

end
