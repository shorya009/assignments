SET AUTOCOMMIT = 0
START TRANSACTION
UPDATE Accounts SET `Balance` = Balance + 1000 WHERE `Account_no`=1234
COMMIT

START TRANSACTION
UPDATE Accounts SET `Balance` = Balance - 500 WHERE `Account_no`=1234
COMMIT

START TRANSACTION
UPDATE Accounts SET `Balance` = CASE WHEN `Account_no` = 1234 THEN balance-200
WHEN `Account_no` = 1235 THEN balance+200
END
COMMIT
